{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "ESLint Configuration",
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "noEmit": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.js",
    "src/**/*.jsx",
    "*.ts",
    "*.tsx",
    "*.js",
    "*.jsx",
    "*.cjs",
    "*.mjs"
  ]
}
